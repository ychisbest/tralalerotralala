---
// Import necessary components and utilities
import Formalizer from "@/components/Formalizer";
import { SITE_TITLE } from "@/consts";
import Image from "astro/components/Image.astro";
import { Icon } from "astro-icon/components";
import { DEFAULT_LOCALE, LOCALES, useTranslations, type Lang } from "@/i18n";
import Layout from "@/layouts/Base.astro";
import image1 from "@/assets/1.png";
import image2 from "@/assets/2.png";

const locale = Astro.currentLocale as Lang;
const t = useTranslations(locale);

// Generate static paths for locales
export const getStaticPaths = () =>
  Object.keys(LOCALES).map((lang) => ({ params: { lang } }));


const currentLocale = locale as Lang;

const canonical = currentLocale === DEFAULT_LOCALE ? (Astro.site?.toString() ?? '') : (Astro.url?.toString() ?? '');
